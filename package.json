    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "@babel/runtime": "^7.13.10",
    "babel-loader": "^8.2.2",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "react": "^18.1.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
